{
  "name": "@relaystack/sdk",
  "version": "0.1.0",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "description": "RelayStack SDK - Unified AI Gateway for OpenAI, Anthropic, Google, and more",
  "author": "RelayStack",
  "license": "MIT",
  "main": "./dist/index.js",
